    "svelte-check": "^4.2.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.2.5",
    "typescript": "^5",
    "vite": "^6.2.4",
    "vitest": "^4.0.3"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "oxlint",
      "bun x ultracite fix"
